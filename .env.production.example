# ðŸš€ Render.com Production Environment Variables
# Copy this file and set actual values in Render.com dashboard

# Rails Configuration
RAILS_ENV=production
RAILS_SERVE_STATIC_FILES=true
RAILS_LOG_TO_STDOUT=true
RAILS_MASTER_KEY=your_master_key_here

# Database (Automatically set by Render.com)
# DATABASE_URL=postgresql://user:password@host:port/database

# Redis (Automatically set by Render.com)
# REDIS_URL=redis://host:port

# Application Settings
SECRET_KEY_BASE=generate_with_rails_secret
WEB_CONCURRENCY=1
RAILS_MAX_THREADS=5

# Demo Mode Settings
DEMO_MODE_ENABLED=true
DEMO_UPDATE_INTERVAL=2000
DEMO_PERFORMANCE_MODE=optimized

# API Settings
API_ACCESS_KEY=your_secure_api_key_here
ALLOWED_ORIGINS=https://dentalsystem.onrender.com

# Monitoring
ENABLE_PERFORMANCE_MONITORING=true
ERROR_REPORTING_LEVEL=info

# AI Integration
AI_SERVICE_ENABLED=true
AI_EFFICIENCY_TARGET=98.5
PREDICTION_ACCURACY_TARGET=99.2

# CORS Settings
PRODUCTION_FRONTEND_URL=https://dentalsystem.onrender.com
DEMO_FRONTEND_URL=https://dentalsystem-demo.onrender.com
# Production Environment Variables Template
# これらの値はRailwayで設定してください

RAILS_ENV=production
RAILS_SERVE_STATIC_FILES=true
RAILS_LOG_TO_STDOUT=true

# Secret Keys (Railwayで設定)
SECRET_KEY_BASE=
RAILS_MASTER_KEY=

# Database (Railwayで自動設定)
DATABASE_URL=

# Redis (Railwayで自動設定)
REDIS_URL=

# External APIs (必要に応じて設定)
LINE_CHANNEL_SECRET=
LINE_CHANNEL_ACCESS_TOKEN=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Application Settings
RAILS_MAX_THREADS=5
DATABASE_POOL_SIZE=5
WEB_CONCURRENCY=2